manifest {
    homePage = 'https://github.com/iarcbioinfo/facets-nf'
    description = ''
    mainScript = 'facets.nf'
}

process {
    container = 'iarcbioinfo/facets-nf'
    conda = ''
    shell =  ['/bin/bash','-o','pipefail']
}

timeline {
  enabled = true
  file = "${params.output_folder}/nf-pipeline_info/facets-nf_timeline.html"
}

report {
  enabled = true
  file = "${params.output_folder}/nf-pipeline_info/facets-nf_report.html"
}

trace {
  enabled = true
  file = "${params.output_folder}/nf-pipeline_info/facets-nf_trace.txt"
}

